{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
	"brace-style": 2,
    "consistent-return": 2,
    "strict": 0,
    "quotes": [0, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
	"no-unused-vars": 0
  }
}
